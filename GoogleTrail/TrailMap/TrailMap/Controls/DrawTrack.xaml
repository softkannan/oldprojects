<UserControl x:Class="TrailMap.Controls.DrawTrack"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="160">
    
    <Grid x:Name="LayoutRoot" Background="White">
    	<ListBox x:Name="listOperations" Height="98" Margin="8,8,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" Width="35"/>
    	<Button x:Name="bttnOpen" Content="Open" Height="26" Margin="65,13,17,0" VerticalAlignment="Top"/>
    	<Button x:Name="bttnSave" Content="Save" Height="30" Margin="65,43,17,0" VerticalAlignment="Top"/>
    	<ListBox x:Name="listPoints" Margin="8,110,8,8"/>

    </Grid>
</UserControl>
